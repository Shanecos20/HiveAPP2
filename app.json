{
  "expo": {
    "name": "HiveApp",
    "slug": "hiveapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFDF5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFDF5"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A smart beekeeping management app for monitoring hive health and receiving AI-powered insights.",
    "primaryColor": "#FFC107"
  }
}
